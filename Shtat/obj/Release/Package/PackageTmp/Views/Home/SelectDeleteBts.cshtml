@model Shtat.Models.BazTarStavka

<div class="modal-header">
    <button class="close" data-dismiss="modal" area-hidden="true"><i class="fa fa-close"></i></button>
    <h5 style="text-align:center">Удаление</h5>
</div>
<div class="modal-body" style="text-align: center">


    <h5>Вы точно хотите удалить БТС "@Model.Gruppa"?</h5>

</div>
<div class="modal-footer" style="text-align: center">
    <button type="submit" class="btn btn-success" style="min-width:100px" onclick="DeleteBts(@Model.IDStavka)">Да</button>
    <button type="button" class="btn btn-default" data-dismiss="modal" style="min-width:100px">Нет</button>
</div>
