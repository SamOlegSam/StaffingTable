@model Shtat.Models.Razr

<div class="modal-header">
    <h5> Создание межразрядного коэффициента</h5>
</div>
<div class="modal-body">
    <label>Разряд</label>
    <input type="text" class="form-control btn-block" id="razr" />

    <label>Разряд по ЕТС</label>
    <input type="text" class="form-control btn-block" id="ETC" />

    <label>Межразрядный коэффициент</label>
    <input type="text" class="form-control btn-block" id="mk" />

    <label>Дата ввода</label>
    <input type="text" class="form-control btn-block" id="datvvoda" />
</div>
<div class="modal-footer" style="text-align:center">
    <button type="submit" class="btn btn-success" style="min-width:100px" onclick="RazrAddSave()">Сохранить</button>
    <button type="button" class="btn btn-default" data-dismiss="modal" style="min-width:100px">Отмена</button>
</div>
